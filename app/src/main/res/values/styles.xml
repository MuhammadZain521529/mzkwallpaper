<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="category_button_style">

        <item name="android:layout_width">@dimen/_70sdp</item>
        <item name="android:layout_height">@dimen/_25sdp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_margin">@dimen/_4sdp</item>
        <item name="android:padding">@dimen/_2sdp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/text_color_selector</item>
        <item name="android:background">@drawable/buttun_bg_selector</item>





    </style>

    <style name="BottomSheetTheme" parent="Theme.Design.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/BottomSheetStyle</item>
    </style>
    <style name="BottomSheetStyle" parent="Widget.Design.BottomSheet.Modal"></style>

</resources>